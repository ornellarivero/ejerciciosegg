//Algoritmo CON_ERRORES
//	Definir vector Como Entero
//	Dimension vector()
//	Para i<-0 Hasta 5 Con Paso 1 Hacer
//		vector(0)=0
//	Fin Para
//FinAlgoritmo

Algoritmo DETECCION_DE_ERRORES
		Definir vector, i Como Entero
		Dimension vector(6)
		Para i<-0 Hasta 5 Con Paso 1 Hacer
			vector(i)=i
		Fin Para
		
		Para i<-0 Hasta 5 Con Paso 1 Hacer
			Escribir vector(i)
		Fin Para
FinAlgoritmo
